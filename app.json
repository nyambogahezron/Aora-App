{
  "expo": {
    "name": "Aora",
    "slug": "aora",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-small.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.jsm.aora"
    },
    "android": {
      "package": "com.jsm.aora",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-small.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-document-picker",
        {
          "permission": "Allow $(PRODUCT_NAME) to access your files."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3519bd06-6276-4e06-8e4f-e7bd1ec02d49"
      }
    }
  }
}
